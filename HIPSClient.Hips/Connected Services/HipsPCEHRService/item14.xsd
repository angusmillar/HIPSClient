        </xs:annotation>
      </xs:element>
      <xs:element xmlns:q2="http://nehta.hips/2014/03" minOccurs="0" name="ValidatedIhi" nillable="true" type="q2:ValidatedIhi">
        <xs:annotation>
          <xs:appinfo />
          <xs:documentation>&lt;summary&gt;
