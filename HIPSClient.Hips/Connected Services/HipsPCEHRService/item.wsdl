  <wsdl:portType name="IPCEHRService">
    <wsdl:documentation>&lt;summary&gt;
            Web service interface for the PCEHR service version 1.
            Contains methods which interact with the National PCEHR Service to gain Digital Health Record access for an Individual, check if the patient has
            a advertised Digital Health Record, refresh the patient's participation status, retrieve a list of documents for a patient from the PCEHR Repository, 
            download a document from the PCEHR, view a patients PCEHR, upload or supersede a document in the PCEHR or remove a document from the PCEHR.
            &lt;/summary&gt;</wsdl:documentation>
    <wsdl:operation name="GainAccessEmergency">
      <wsdl:documentation>&lt;summary&gt;
            Requests emergency access to the patient's records on the PCEHR
            &lt;/summary&gt;
            &lt;param name="patientIdentifier"&gt;Patient identifier (Hospital-level MRN, State Patient ID, Validated IHI or PCEHR Data Store PatientMasterId)&lt;/param&gt;
            &lt;param name="user"&gt;The health provider individual, interactive user or authorised employee responsible for this action.&lt;/param&gt;
            &lt;returns&gt;Gain PCEHR Access response.&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessEmergency" message="tns:IPCEHRService_GainAccessEmergency_InputMessage" />
      <wsdl:output wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessEmergencyResponse" message="tns:IPCEHRService_GainAccessEmergency_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="GainAccessWithCode">
      <wsdl:documentation>&lt;summary&gt;
            Request access with the access code being sent to the PCEHR
            &lt;/summary&gt;
            &lt;param name="patientIdentifier"&gt;Patient identifier (Hospital-level MRN, State Patient ID, Validated IHI or PCEHR Data Store PatientMasterId)&lt;/param&gt;
            &lt;param name="AccessCode"&gt;The code to be used to gain access.&lt;/param&gt;
            &lt;param name="user"&gt;The health provider individual, interactive user or authorised employee responsible for this action.&lt;/param&gt;
            &lt;returns&gt;Gain PCEHR Access response.&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessWithCode" message="tns:IPCEHRService_GainAccessWithCode_InputMessage" />
      <wsdl:output wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessWithCodeResponse" message="tns:IPCEHRService_GainAccessWithCode_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="GainAccessWithOutCode">
      <wsdl:documentation>&lt;summary&gt;
            Request for access without a code being required by PCEHR
            &lt;/summary&gt;
            &lt;param name="patientIdentifier"&gt;Patient identifier (Hospital-level MRN, State Patient ID, Validated IHI or PCEHR Data Store PatientMasterId)&lt;/param&gt;
            &lt;param name="user"&gt;The health provider individual, interactive user or authorised employee responsible for this action.&lt;/param&gt;
            &lt;returns&gt;Gain PCEHR Access response.&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessWithOutCode" message="tns:IPCEHRService_GainAccessWithOutCode_InputMessage" />
      <wsdl:output wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GainAccessWithOutCodeResponse" message="tns:IPCEHRService_GainAccessWithOutCode_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="GetChangeHistoryView">
      <wsdl:documentation>&lt;summary&gt;
            Gets the list of uploaded and changed documents from a single document set
            &lt;/summary&gt;
            &lt;param name="patientIdentifier"&gt;Patient identifier (Hospital-level MRN, State Patient ID, Validated IHI or PCEHR Data Store PatientMasterId)&lt;/param&gt;
            &lt;param name="user"&gt;The health provider individual, interactive user or authorised employee responsible for this action.&lt;/param&gt;
            &lt;param name="documentUniqueId"&gt;The document entry UUID (NOC requirements disallow using the document ID).&lt;/param&gt;
            &lt;returns&gt;List of versions of the document.&lt;/returns&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GetChangeHistoryView" message="tns:IPCEHRService_GetChangeHistoryView_InputMessage" />
      <wsdl:output wsaw:Action="http://schemas.HIPS/Services/2012/01/IPCEHRService/GetChangeHistoryViewResponse" message="tns:IPCEHRService_GetChangeHistoryView_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="GetDocumentList">
      <wsdl:documentation>&lt;summary&gt;
            Gets the list of all avalible documents.
            This function is given the WS name of GetDocumentList.
            &lt;/summary&gt;
            &lt;param name="patientIdentifier"&gt;Patient identifier (Hospital-level MRN, State Patient ID, Validated IHI or PCEHR Data Store PatientMasterId)&lt;/param&gt;
